{
  "expo": {
    "name": "karanvirportfolio",
    "slug": "karanvirportfolio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "cli": {
      "appVersionSource": "remote"
    },
    "build": {
      "production": {
        "autoIncrement": true
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f6778ce3-a092-457a-81dd-a94672284ef3"
      }
    },
    "android": {
      "package": "com.karanvir011998.karanvirportfolio"
    },
    "owner": "karanvir011998"
  }
}